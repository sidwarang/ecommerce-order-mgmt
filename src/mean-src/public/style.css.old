@import url(http://fonts.googleapis.com/css?family=Oxygen+Mono);
@charset "UTF-8";
#wrapper {
  float: left;
  width:200px;
}
.navmenu {
  
  width:410px;
}
.navmenu ul,
.navmenu ul li,
.navmenu ul ul {
  list-style: none;
  margin: 0;
  padding: 0;
}
.navmenu ul {
  position: relative;
  z-index: 597;
  float: left;
}
.navmenu ul li {
  float: left;
  min-height: 1px;
  line-height: 1em;
  vertical-align: middle;
  position: relative;
}
.navmenu ul li.hover,
.navmenu ul li:hover {
  position: relative;
  z-index: 599;
  cursor: default;
}
.navmenu ul ul {
  visibility: hidden;
  position: absolute;
  top: 100%;
  left: 0px;
  z-index: 598;
  width: 100%;
}
.navmenu ul ul li {
  float: none;
}
.navmenu ul ul ul {
  top: -2px;
  right: 0;
}
.navmenu ul li:hover > ul {
  visibility: visible;
}
.navmenu ul ul {
  top: 1px;
  left: 99%;
}
.navmenu ul li {
  float: none;
}
.navmenu ul ul {
  margin-top: 1px;
}
.navmenu ul ul li {
  font-weight: normal;
}
/* Custom CSS Styles */
.navmenu {
  width: 200px;
  background: #333333;
  font-family: 'Oxygen Mono', Tahoma, Arial, sans-serif;
  zoom: 1;
  font-size: 12px;
}
.navmenu:before {
  content: '';
  display: block;
}
.navmenu:after {
  content: '';
  display: table;
  clear: both;
}
.navmenu a {
  display: block;
  padding: 15px 20px;
  color: #ffffff;
  text-decoration: none;
  text-transform: uppercase;
}
.navmenu > ul {
  width: 200px;
}
.navmenu ul ul {
  width: 200px;
}
.navmenu > ul > li > a {
  border-right: 4px solid #1b9bff;
  color: #ffffff;
}
.navmenu > ul > li > a:hover {
  color: #ffffff;
}
.navmenu > ul > li.active a {
  background: #1b9bff;
}
.navmenu > ul > li a:hover,
.navmenu > ul > li:hover a {
  background: #1b9bff;
}
.navmenu li {
  position: relative;
}
.navmenu ul li.has-sub > a:after {
  content: '+';
  position: absolute;
  top: 50%;
  right: 15px;
  margin-top: -6px;
}
.navmenu ul ul li.first {
  -webkit-border-radius: 0 3px 0 0;
  -moz-border-radius: 0 3px 0 0;
  border-radius: 0 3px 0 0;
}
.navmenu ul ul li.last {
  -webkit-border-radius: 0 0 3px 0;
  -moz-border-radius: 0 0 3px 0;
  border-radius: 0 0 3px 0;
  border-bottom: 0;
}
.navmenu ul ul {
  -webkit-border-radius: 0 3px 3px 0;
  -moz-border-radius: 0 3px 3px 0;
  border-radius: 0 3px 3px 0;
}
.navmenu ul ul {
  border: 1px solid #0082e7;
}
.navmenu ul ul a {
  font-size: 12px;
  color: #ffffff;
}
.navmenu ul ul a:hover {
  color: #ffffff;
}
.navmenu ul ul li {
  border-bottom: 1px solid #0082e7;
}
.navmenu ul ul li:hover > a {
  background: #4eb1ff;
  color: #ffffff;
}
.navmenu.align-right > ul > li > a {
  border-left: 4px solid #1b9bff;
  border-right: none;
}
.navmenu.align-right {
  float: right;
}
.navmenu.align-right li {
  text-align: right;
}
.navmenu.align-right ul li.has-sub > a:before {
  content: '+';
  position: absolute;
  top: 50%;
  left: 15px;
  margin-top: -6px;
}
.navmenu.align-right ul li.has-sub > a:after {
  content: none;
}
.navmenu.align-right ul ul {
  visibility: hidden;
  position: absolute;
  top: 0;
  left: -100%;
  z-index: 598;
  width: 100%;
}
.navmenu.align-right ul ul li.first {
  -webkit-border-radius: 3px 0 0 0;
  -moz-border-radius: 3px 0 0 0;
  border-radius: 3px 0 0 0;
}
.navmenu.align-right ul ul li.last {
  -webkit-border-radius: 0 0 0 3px;
  -moz-border-radius: 0 0 0 3px;
  border-radius: 0 0 0 3px;
}
.navmenu.align-right ul ul {
  -webkit-border-radius: 3px 0 0 3px;
  -moz-border-radius: 3px 0 0 3px;
  border-radius: 3px 0 0 3px;
}

.content {
  width:1670px;
  float:right;
  padding-left:24px;
}